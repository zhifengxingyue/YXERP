@{
    ViewBag.Title = "模块列表";
}
<div class="headertitle">
    <span class="header-title">模块列表</span>
</div>
<table class="table-list mTop20">
    <tr class="tr-header">
        <td>名称</td>
        <td>描述</td>
    </tr>
    @foreach (CloudSalesEntity.Modules model in (List<CloudSalesEntity.Modules>)ViewBag.Items)
    {
         <tr>
            <td>@(model.Name)</td>
            <td>@(model.Description)</td>
        </tr>
    }
</table>


