@{
    ViewBag.Title = "部门管理";
}

@section css{
    <link rel="stylesheet" href="/modules/css/system/organization.css" />
}
@section scripts{
    <script type="text/javascript">
        seajs.use(["scripts/organization/department"], function (obj) {
            obj.init();
        });
    </script>
}
<div class="header-box">
    <span class="header-title left">部门管理</span>
    <div class="btn mLeft20" id="createModel">创建部门</div>
</div>
<div class="content-body">
    <table class="table-list">
        <tr class="tr-header">
            <td>名称</td>
            <td>描述</td>
            <td class="width150">创建时间</td>
            <td class="width150">创建人</td>
            <td class="width80">编辑</td>
            <td class="width80">删除</td>
        </tr>
    </table>
</div>
