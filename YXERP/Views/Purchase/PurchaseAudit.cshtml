@{
    ViewBag.Title = "采购审核";
}

<link href="/modules/css/orders/purchase.css" rel="stylesheet" />
@section scripts{
    <script type="text/javascript">
        seajs.use(["scripts/purchase/mypurchase"], function (obj) {
            obj.init(true);
        });
    </script>
}
<div class="header-box">
    <span class="header-title left">采购审核</span>
    <a href="/Purchase/Purchase" class="ico-add left">新建采购</a>
    <div class="right searth-module"> </div> 
</div>
<div class="content-body">
    <table class="table-list">
        <tr class="tr-header">
            <td>采购单号</td>
            <td>状态</td>
            <td>总金额</td>
            <td>采购员</td>
            <td>采购日期</td>
            <td>操作</td>
        </tr>
    </table>
</div>
<ul class="dropdown-ul hide" style="width:70px;">
    <li id="audit">审核</li>
    <li id="invalid">作废</li>
</ul>
