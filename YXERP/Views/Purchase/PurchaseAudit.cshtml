@{
    ViewBag.Title = "采购审核";
}

<link href="/modules/css/orders/purchase.css" rel="stylesheet" />
@section scripts{
    <script type="text/javascript">
        seajs.use(["scripts/purchase/mypurchase"], function (obj) {
            obj.init(true);
        });
    </script>
}
<div class="header-box">
    <span class="header-title left">采购审核</span>
    <a href="/Purchase/Purchase" class="ico-add left">新建采购</a>
    <div class="right searth-module"> </div> 
</div>
<div class="content-body">
    <table class="table-list">
        <tr class="tr-header">
            <td class="tLeft">采购单号</td>
            <td class="width100">状态</td>
            <td class="tRight">总金额</td>
            <td class="width150">采购员</td>
            <td class="width200">采购日期</td>
            <td class="width80">操作</td>
        </tr>
    </table>
</div>
<ul class="dropdown-ul hide" style="width:65px;">
    <li id="audit">审核</li>
    <li id="invalid">作废</li>
</ul>
