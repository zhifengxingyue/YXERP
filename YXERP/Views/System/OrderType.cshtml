@{
    ViewBag.Title = "销售订单类型配置";
}

@section css{
    <link rel="stylesheet" href="/modules/css/system/system.css" />
}
@section scripts{
    <script type="text/javascript">
        seajs.use(["scripts/system/ordertype"], function (obj) {
            obj.init();
        });
    </script>
}
<div class="header-box">
    <span class="header-title left">销售订单类型配置</span>
    <div class="ico-add" id="createModel">新建类型</div>
</div>
<div class="content-body">
    <table class="table-list">
        <tr class="tr-header">
            <td>类型名称</td>
            <td>类型编码</td>
            <td class="width150">创建时间</td>
            <td class="width150">创建人</td>
            <td class="width80">操作</td>
        </tr>
    </table>
</div>
<ul class="dropdown-ul hide"  style="width:65px;">
    <li id="updateObject">编辑</li>
    <li id="deleteObject">删除</li>
</ul>
