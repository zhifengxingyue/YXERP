@{
    var model = (CloudSalesEntity.WareHouse)ViewBag.Ware;
    ViewBag.Title = "货位设置";
    
}

<link href="/modules/css/system/system.css" rel="stylesheet" />
@section scripts{
    <script type="text/javascript">
        seajs.use(["scripts/system/depotseat"], function (obj) {
            obj.init('@(model.WareID)');
        });
    </script>
}
<div class="header-box">
    <span class="header-title left">@(model.Name)-货位设置</span>
    <a href="javascript:void(0);" class="ico-add left">新建货位</a>
    <a href="/System/Warehouse" class="ico-back left">返回仓库设置</a>
    <div class="right searth-module"> </div> 
</div>

<div class="content-body">
    <table class="table-list">
        <tr class="tr-header" id="warehouse-items">
            <td class="width150">货位编码</td>
            <td>货位名称</td>
            <td>仓库名称</td>
            <td class="width100">启用/禁用</td>
            <td class="width300">描述</td>
            <td class="width100">操作</td>
        </tr>
    </table>
    <div id="pager" class="mTop20"></div>
</div>
<ul class="dropdown-ul hide"  style="width:65px;">
    <li id="updateObject">编辑</li>
    <li id="deleteObject">删除</li>
</ul>
